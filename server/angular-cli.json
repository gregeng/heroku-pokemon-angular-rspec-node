{
  "project": {
    "version": "1.0.0-beta.30",
    "name": "client"
  },
  "apps": [
    {
      "root": "",
      "outDir": "dist",
      "assets": [
        "client/src/assets",
        "client/src/favicon.ico"
      ],
      "index": "client/src/index.html",
      "main": "client/src/main.ts",
      "polyfills": "client/src/polyfills.ts",
      "test": "client/src/test.ts",
      "tsconfig": "client/src/tsconfig.json",
      "prefix": "app",
      "styles": [
        "client/node_modules/bootstrap/dist/css/bootstrap.css",
        "client/src/styles.scss"
      ],
      "scripts": [
        "client/node_modules/jquery/dist/jquery.js",
        "client/node_modules/tether/dist/js/tether.js",
        "client/node_modules/bootstrap/dist/js/bootstrap.js"
      ],
      "environments": {
        "source": "client/src/environments/environment.ts",
        "dev": "client/src/environments/environment.dev.ts",
        "test": "client/src/environments/environment.test.ts",
        "prod": "client/src/environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "client/protractor.conf.js"
    }
  },
  "lint": [
    {
      "files": "client/src/**/*.ts",
      "project": "client/src/tsconfig.json"
    },
    {
      "files": "client/e2e/**/*.ts",
      "project": "client/e2e/tsconfig.json"
    }
  ],
  "test": {
    "karma": {
      "config": "client//karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
